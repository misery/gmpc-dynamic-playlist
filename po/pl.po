#: plugin.c:295
msgid "Dynamic search cannot find a new song"
msgstr "Dynamiczne wyszukiwanie nie zwróciło nowych utworów"

#: plugin.c:447
msgid "Dynamic search is already busy"
msgstr "Dynamiczne wyszukiwanie w toku"

#: plugin.c:454
msgid "You need to play a song that will be used"
msgstr "Musisz odtworzyć utwory, które zostaną użyte"

#: plugin.c:498
msgid "Cannot prune playlist! There is no current song as position"
msgstr "Nie można skrócić listy odtwarzania! Nie ma utwory jako pozycji"

#: plugin.c:552
msgid "prune"
msgstr "skrócona"

#: plugin.c:552
msgid "Prune playlist"
msgstr "Skróć listę odtwarzania"

#: plugin.c:553
msgid "dynamic"
msgstr "dynamiczna"

#: plugin.c:553
msgid "Dynamic search (on|off)"
msgstr "Dynamiczne wyszukiwanie (wł|wył)"

#: plugin.c:554
msgid "similar"
msgstr "podobna"

#: plugin.c:554
msgid "Search for similar song/artist/genre"
msgstr "Wyszukaj podobne utwory/wykonawców/gatunki"

#: plugin.c:596
msgid "Dynamic search"
msgstr "Dynamiczne wyszukiwanie"

#: plugin.c:601
msgid "Add similar song"
msgstr "Dodaj podobny utwór"

#: plugin.c:719
msgid "<b>Meta-Data options</b>"
msgstr "<b>Ustawienia metadanych</b>"

#: plugin.c:723
msgid "Search songs in 'similar songs'"
msgstr "Wyszukaj utwory w 'podobnych utworach'"

#: plugin.c:730
msgid "Search max. songs in database:"
msgstr "Maksimum zwracanych wyników wyszukiwania utworów:"

#: plugin.c:740
msgid "Search songs in 'similar artists'"
msgstr "Wyszukaj utwory w 'podobnych wykonawcach'"

#: plugin.c:746
msgid "Same artist is also 'similar'"
msgstr "Ten sam artysta jest również 'podobny'"

#: plugin.c:754
msgid "Search max. artists in database:"
msgstr "Maksimum zwracanych wyników wyszukiwania wykonawców:"

#: plugin.c:764
msgid "Search songs in 'similar genre'"
msgstr "Wyszukaj utwory w 'podobnych gatunkach'"

#: plugin.c:770
msgid "Same genre is also 'similar'"
msgstr "Ten sam gatunek jest również 'podobny'"

#: plugin.c:777
msgid "Search max. genre in database:"
msgstr "Maksimum zwracanych wyników wyszukiwania gatunków:"

#: plugin.c:789
msgid "<b>Local options</b>"
msgstr "<b>Ustawienia lokalne</b>"

#: plugin.c:793
msgid "Enable dynamic search"
msgstr "Włącz dynamiczne wyszukiwanie"

#: plugin.c:799
msgid "Search songs in same genre"
msgstr "Wyszukaj utwory w tym samym gatunku"

#: plugin.c:806
msgid "Keep x songs after current:"
msgstr "Odtwarzaj x utworów po bieżącym:"

#: plugin.c:817
msgid "Block played song for at least x songs:"
msgstr "Zablokuj odtwarzany utwór dla conajmniej x utworów:"

#: plugin.c:828
msgid "Block played artist for at least x songs:"
msgstr "Zablokuj odtwarzanego wykonawcę dla conajmniej x utworów:"

#: plugin.c:859
msgid "Dynamic Playlist"
msgstr "Dynamiczna lista odtwarzania"
